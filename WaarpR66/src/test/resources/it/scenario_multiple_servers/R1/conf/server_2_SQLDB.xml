<?xml version="1.0" encoding="UTF-8"?>
<config>
  <comment>Configuration file for a server with a H2 database</comment>
  <identity>
    <hostid>server1</hostid>
    <sslhostid>server1-ssl</sslhostid>
    <cryptokey>src/test/resources/it/certs/test-key.des</cryptokey>
  </identity>
  <server>
    <serveradmin>monadmin</serveradmin>
    <serverpasswd>c5f4876737cf351a</serverpasswd>
    <usenossl>True</usenossl>
    <usessl>True</usessl>
    <usehttpcomp>False</usehttpcomp>
    <uselocalexec>False</uselocalexec>
    <httpadmin>src/main/httpadmin/admin-bootstrap</httpadmin>
    <admkeypath>src/test/resources/it/certs/testsslnocert.jks</admkeypath>
    <admkeystorepass>testsslnocert</admkeystorepass>
    <admkeypass>testalias</admkeypass>
    <checkaddress>False</checkaddress>
    <checkclientaddress>False</checkclientaddress>
    <pastlimit>86400000</pastlimit>
    <minimaldelay>5000</minimaldelay>
    <multiplemonitors>2</multiplemonitors>
    <!-- <snmpconfig></snmpconfig> -->
  </server>
  <network>
    <serverport>6676</serverport>
    <serversslport>6677</serversslport>
    <serverhttpport>8076</serverhttpport>
    <serverhttpsport>8077</serverhttpsport>
  </network>
  <ssl>
    <keypath>src/test/resources/it/certs/testsslnocert.jks</keypath>
    <keystorepass>testsslnocert</keystorepass>
    <keypass>testalias</keypass>
    <trustkeypath>src/test/resources/it/certs/testcert.jks</trustkeypath>
    <trustkeystorepass>testcert</trustkeystorepass>
    <trustuseclientauthenticate>True</trustuseclientauthenticate>
  </ssl>
  <directory>
    <serverhome>/tmp/R66/scenario_multiple_servers/R1</serverhome>
    <in>in</in>
    <out>out</out>
    <arch>arch</arch>
    <work>work</work>
    <conf>conf</conf>
  </directory>
  <rest>
    <restaddress>0.0.0.0</restaddress>
    <restport>8098</restport>
    <restssl>true</restssl>
    <restauthenticated>true</restauthenticated>
    <resttimelimit>3000</resttimelimit>
    <restsignature>true</restsignature>
    <restsigkey>src/test/resources/it/certs/key.sha256</restsigkey>
    <restmethod>
      <restname>ALL</restname>
      <restcrud>CRUD</restcrud>
    </restmethod>
    <restmethod>
      <restname>Bandwidth</restname>
      <restcrud>CRUD</restcrud>
    </restmethod>
    <restmethod>
      <restname>Information</restname>
      <restcrud>CRUD</restcrud>
    </restmethod>
    <restmethod>
      <restname>Server</restname>
      <restcrud>CRUD</restcrud>
    </restmethod>
    <restmethod>
      <restname>Control</restname>
      <restcrud>CRUD</restcrud>
    </restmethod>
  </rest>
  <limit>
    <serverthread>16</serverthread>
    <clientthread>500</clientthread>
    <usefastmd5>False</usefastmd5>
    <timeoutcon>30000</timeoutcon>
    <delayretry>1000</delayretry>
    <digest>7</digest>
    <globaldigest>True</globaldigest>
    <localdigest>False</localdigest>
    <delaycommand>500</delaycommand>
    <runlimit>2000</runlimit>
    <blocksize>1048576</blocksize>
    <usecpulimit>False</usecpulimit>
    <usejdkcpulimit>False</usejdkcpulimit>
    <cpulimit>0.99</cpulimit>
    <connlimit>0</connlimit>
  </limit>
  <db>
    <dbdriver>XXXDRIVERXXX</dbdriver>
    <dbserver>XXXJDBCXXX</dbserver>
    <dbuser>test</dbuser>
    <dbpasswd>test</dbpasswd>
    <dbcheck>false</dbcheck>
  </db>
</config>
