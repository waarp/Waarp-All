<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <comment>Example of config file: change its as your need.</comment>
    <identity>
        <hostid>hosta</hostid>
        <sslhostid>hostas</sslhostid>
        <cryptokey>/GG/R66/certs/test-key.des</cryptokey>
        <authentfile>/GG/R66/conf/OpenR66-authent-A-2.xml</authentfile>
    </identity>
    <client>
    </client>
    <ssl>
        <keypath>/GG/R66/certs/testsslnocert.jks</keypath>
        <keystorepass>testsslnocert</keystorepass>
        <keypass>testalias</keypass>
        <trustkeypath>/GG/R66/certs/testcert.jks</trustkeypath>
        <trustkeystorepass>testcert</trustkeystorepass>
        <trustuseclientauthenticate>False</trustuseclientauthenticate>
    </ssl>
    <directory>
        <serverhome>/GG/R66/</serverhome>
        <in>in</in>
        <out>out</out>
        <arch>arch</arch>
        <work>work</work>
        <conf>conf</conf>
    </directory>
    <limit>
        <digest>4</digest>
        <usefastmd5>True</usefastmd5>
        <blocksize>4096</blocksize>
        <timeoutcon>10000</timeoutcon>
        <sessionlimit>0</sessionlimit>
        <globallimit>0</globallimit>
        <delaylimit>10000</delaylimit>
		<checkversion>True</checkversion>
    </limit>
	<business>
		<businessid>hosta</businessid>
		<businessid>hostas</businessid>
	</business>
	<roles>
		<role>
			<roleid>hosta</roleid>
			<roleset>CONFIGADMIN|SYSTEM</roleset>
		</role>
		<role>
			<roleid>hostas</roleid>
			<roleset>FULLADMIN</roleset>
		</role>
	</roles>
</config>
