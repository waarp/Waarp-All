<?xml version="1.0" encoding="UTF-8"?>
<config xmlns:x0="http://www.w3.org/2001/XMLSchema">
  <identity>
    <hostid>hosta</hostid>
    <sslhostid>hostas</sslhostid>
    <cryptokey>J:\GG\R66\certs\test-key.des</cryptokey>
    <authentfile>J:\GG\R66\conf\OpenR66-authent-A-2.xml</authentfile>
  </identity>
  <ssl>
    <keypath>J:\GG\R66\certs\testsslnocert.jks</keypath>
    <keystorepass>testsslnocert</keystorepass>
    <keypass>testalias</keypass>
    <trustkeypath>J:\GG\R66\certs\testcert.jks</trustkeypath>
    <trustkeystorepass>testcert</trustkeystorepass>
    <trustuseclientauthenticate>True</trustuseclientauthenticate>
  </ssl>
  <directory>
    <serverhome>J:/GG/R66</serverhome>
    <in>in</in>
    <out>out</out>
    <arch>arch</arch>
    <work>work</work>
    <conf>conf</conf>
  </directory>
  <limit>
    <digest>7</digest>
    <usefastmd5>False</usefastmd5>
    <delayretry>10000</delayretry>
    <timeoutcon>10000</timeoutcon>
    <serverthread>10</serverthread>
    <clientthread>100</clientthread>
    <memorylimit>4000000000</memorylimit>
    <sessionlimit>0</sessionlimit>
    <globallimit>0</globallimit>
    <delaylimit>10000</delaylimit>
    <runlimit>10000</runlimit>
    <delaycommand>5000</delaycommand>
    <blocksize>65536</blocksize>
    <gaprestart>30</gaprestart>
    <usenio>False</usenio>
    <usecpulimit>False</usecpulimit>
    <usejdkcpulimit>False</usejdkcpulimit>
    <cpulimit>1.0</cpulimit>
    <connlimit>0</connlimit>
    <lowcpulimit>0.75</lowcpulimit>
    <highcpulimit>0.0</highcpulimit>
    <percentdecrease>0.05</percentdecrease>
    <delaythrottle>1000</delaythrottle>
    <limitlowbandwidth>10000000</limitlowbandwidth>
    <checkversion>True</checkversion>
  </limit>
  <db>
    <dbdriver>h2</dbdriver>
    <dbserver>
      jdbc:h2:J:/GG/R66/data/openr66;IFEXISTS=TRUE;MODE=Oracle;AUTO_SERVER=TRUE
    </dbserver>
    <dbuser>openr66</dbuser>
    <dbpasswd>openr66</dbpasswd>
  </db>
  <business>
    <businessid>hosta</businessid>
    <businessid>hostas</businessid>
  </business>
  <spooleddaemon>
    <stopfile>J:\GG\R66\test\stopout.txt</stopfile>
    <spooled>
      <name>direct</name>
      <to>hostas</to>
      <to>aliasas</to>
      <rule>rulespooled</rule>
      <statusfile>J:\GG\R66\test\statusoutdirect.json</statusfile>
      <directory>J:\GG\R66\test\out</directory>
      <directory>J:\GG\R66\test\out2</directory>
      <regex>.*\.?ar$</regex>
      <recursive>True</recursive>
      <elapse>1000</elapse>
      <submit>False</submit>
      <parallel>True</parallel>
      <limitParallel>0</limitParallel>
      <info>spooled transfer</info>
      <waarp>hostas</waarp>
      <elapseWaarp>5000</elapseWaarp>
    </spooled>
    <spooled>
      <name>submit</name>
      <to>hostas</to>
      <rule>rulespooled</rule>
      <statusfile>J:\GG\R66\test\statusoutsubmit.json</statusfile>
      <directory>J:\GG\R66\test\out</directory>
      <directory>J:\GG\R66\test\out2</directory>
      <regex>.*\.?ar$</regex>
      <recursive>True</recursive>
      <elapse>1000</elapse>
      <submit>True</submit>
      <info>spooled submit</info>
      <waarp>hostas</waarp>
      <elapseWaarp>5000</elapseWaarp>
    </spooled>
  </spooleddaemon>
</config>
