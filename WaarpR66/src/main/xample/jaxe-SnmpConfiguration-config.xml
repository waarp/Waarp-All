    <AFFICHAGE_ELEMENT type="typesimple" element="trustkeypath"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="trustkeystorepass"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="admkeypath"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="admkeystorepass"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="admkeypass"/>
    <AFFICHAGE_ELEMENT type="division" element="snmpconfig"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="comment"/>
    <AFFICHAGE_ELEMENT type="zone" element="config"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="localaddress"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="nbthread"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="filtered"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="usetrap"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="trapinformlevel"/>
    <AFFICHAGE_ELEMENT type="zone" element="targets"/>
    <AFFICHAGE_ELEMENT type="zone" element="target"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="name"/>
    <AFFICHAGE_ELEMENT type="typesimple" element="domain"/>
