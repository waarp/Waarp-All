      <MENU_INSERTION nom="keypath"/>
      <MENU_INSERTION nom="keystorepass"/>
      <MENU_INSERTION nom="keypass"/>
      <MENU_INSERTION nom="trustkeypath"/>
      <MENU_INSERTION nom="trustkeystorepass"/>
      <MENU_INSERTION nom="admkeypath"/>
      <MENU_INSERTION nom="admkeystorepass"/>
      <MENU_INSERTION nom="admkeypass"/>
      <MENU_INSERTION nom="tasks"/>
      <MENU_INSERTION nom="task"/>
      <MENU_INSERTION nom="type"/>
      <MENU_INSERTION nom="path"/>
      <MENU_INSERTION nom="delay"/>
      <MENU_INSERTION nom="comment"/>
      <MENU_INSERTION nom="idrule"/>
      <MENU_INSERTION nom="hostids"/>
      <MENU_INSERTION nom="hostid"/>
      <MENU_INSERTION nom="mode"/>
      <MENU_INSERTION nom="recvpath"/>
      <MENU_INSERTION nom="sendpath"/>
      <MENU_INSERTION nom="archivepath"/>
