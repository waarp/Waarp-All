<?xml version="1.0" encoding="UTF-8"?>

<config>
  <comment>Example of config file: change it as your need.</comment>
  <identity>
    <hostid>hostb</hostid>
    <sslhostid>hostbs</sslhostid>
    <cryptokey>src/test/resources/r66/certs/test-key.des</cryptokey>
    <authentfile>src/test/resources/r66/OpenR66-authent-ThroughProxy.xml
    </authentfile>
  </identity>
  <client>
    <taskrunnernodb>True</taskrunnernodb>
  </client>
  <ssl>
    <keypath>src/test/resources/r66/certs/testsslnocert.jks</keypath>
    <keystorepass>testsslnocert</keystorepass>
    <keypass>testalias</keypass>
    <trustkeypath>src/test/resources/r66/certs/testcert.jks</trustkeypath>
    <trustkeystorepass>testcert</trustkeystorepass>
    <trustuseclientauthenticate>False</trustuseclientauthenticate>
  </ssl>
  <directory>
    <serverhome>/tmp/R66</serverhome>
    <in>in</in>
    <out>out</out>
    <arch>arch</arch>
    <work>work</work>
    <conf>conf</conf>
  </directory>
  <limit>
    <usefastmd5>False</usefastmd5>
    <digest>7</digest>
    <checkversion>True</checkversion>
    <timeoutcon>1000</timeoutcon>
    <delayretry>1000</delayretry>
    <runlimit>500</runlimit>
    <clientthread>500</clientthread>
  </limit>
</config>
